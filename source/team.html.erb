<div id="team" class="container text-center padded">
<h1>Our team</h1>
<ul class="list-inline text-center">
  <% ["bruno", "pedro"].each do |member| %>
    <li>
      <h2><%= member.capitalize %></h2>
      <%= image_tag "#{member}.jpg", class: "avatar-large" %>
    </li>
  <% end %>
</ul>
<h3 class="padded">
    <% link_to "mailto:contact@airbnb.com" do %>
      <i class="fa fa-envelope"> contact@airbnb.com</i>
    <% end %>
  </h3>
</div>
